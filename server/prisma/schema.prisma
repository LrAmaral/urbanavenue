generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Products {
  id    String @id @default(uuid())
  type  String
  title String
  url   String
  size  String
  image String
  price Float
  desc  String
  stock Int
}

model Users {
  id    String @id @default(uuid())
  name  String
  email String @unique
}
