// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Products {
  id     String   @id @default(auto()) @map("_id") @db.ObjectId
  type   String
  title  String
  url    String
  size   String
  image  String
  price  Float
  desc   String
  stock  Int
}

model Users {
  id     String   @id @default(auto()) @map("_id") @db.ObjectId
  name   String
  email  String   @unique
}
